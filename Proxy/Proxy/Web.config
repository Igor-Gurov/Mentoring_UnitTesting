<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>
	<system.webServer>
		<handlers>
			<add verb="*" path="*.prx" name="ProxyHttpHandler" type="Proxy.ProxyHttpHandler"/>
		</handlers>
		<defaultDocument>
			<files>
				<add value="MainPage.aspx"/>
			</files>
		</defaultDocument>
	</system.webServer>
</configuration>
